	<form id="frmBusqueda" method="get" action="/quizes">
		<div id="dvContenedor">
			<div id="dvCabeceraBuscador">
				<h4>Buscador<h4>
			</div>
  			<div>
			<label for="search">Pregunta:</label>
  			<input type="text" id="search" name="search" size="33" />
			</div>
 			<div>
  			<input type="submit" value="Buscar" />
			</div>
		</div>
	</form>
</br>
<% if(quizes.length > 0) {%>
<table id="lstPreguntas">
<tr>
	<th> Preguntas </th>
</tr>
<% var i; for(i = 0; i < quizes.length; i++) {%>
	<tr>
		<td>
			<a href="/quizes/<%=quizes[i].id%>"><%=quizes[i].pregunta%></a>
		</td>
	</tr>
<% } %>
</table>
<% } else { %>
